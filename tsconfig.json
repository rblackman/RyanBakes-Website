{
	"compilerOptions": {
		"allowJs": true,
		"downlevelIteration": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"incremental": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"lib": [
			"dom",
			"dom.iterable",
			"esnext"
		],
		"module": "esnext",
		"moduleResolution": "node",
		"noEmit": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "es5",
		"typeRoots": [
			"node_modules/@types",
			"@types"
		],
		"types": [
			"react",
			"next",
			"workbox-window",
			"workbox-strategies",
			"workbox-routing",
			"workbox-expiration",
		]
	},
	"exclude": [
		"node_modules"
	],
	"include": [
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx"
	]
}
